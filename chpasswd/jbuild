(jbuild_version 1)

(library (
  (name doveadm)
  (public_name doveadm)
  (synopsis "OCaml interface to doveadm")
  (libraries (unix))
  (modules doveadm)
  (wrapped false)
))

(executable (
  (name doveadmTest)
  (libraries (doveadm kaputt))
  (modules doveadmTest)
))

(library (
  (name webapp)
  (libraries (doveadm sqlite3 cryptokit netcgi2 jingoo calendar))
  (modules (database model token config view mails controller template))
  (wrapped false)
))
